{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/page.js",
    "methods":["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
    "headers":{
      "Access-Contol-Allow-Origin":"*"
    }
    }
  ]
}